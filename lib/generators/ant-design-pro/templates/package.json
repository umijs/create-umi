{
  "name": "ant-design-pro",
  "version": "4.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js",
    "lint-staged:ts": "tslint",
    "lint:fix": "eslint --fix --ext .js src tests && npm run lint:style && npm run tslint:fix",
    "lint:js": "eslint --ext .js src tests",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix 'src/**/*.less' --syntax less",
    "lint:ts": "tslint -p . -c tslint.yml",
    "prettier": " check-prettier write",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "tslint:fix": "tslint --fix 'src/**/*.ts*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx,md,json,jsx,less}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{ts,tsx}": "npm run lint-staged:ts"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/pro-layout": "^4.2.0",
    "antd": "^3.16.1",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "qs": "^6.7.0",
    "rc-animate": "^2.4.4",
    "react": "^16.8.5",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@types/puppeteer": "^1.12.4",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "^16.0.11",
    "babel-eslint": "^10.0.1",
    "check-prettier": "^1.0.1",
    "cross-env": "^5.2.0",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.2.0",
    "jest-puppeteer": "^4.1.0",
    "jsdom-global": "^3.0.2",
    "less": "^3.9.0",
    "lint-staged": "^8.1.1",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.17.0",
    "slash2": "^2.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-rational-order": "^0.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^2.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.6.17",
    "umi-plugin-pro-block": "^1.3.0",
    "umi-plugin-react": "^1.7.6",
    "umi-request": "^1.0.5",
    "umi-types": "^0.3.3"
  },
  "optionalDependencies": {
    "puppeteer": "^1.12.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}